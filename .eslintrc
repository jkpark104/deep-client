{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb", "eslint:recommended", "next", "next/core-web-vitals", "prettier"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react/jsx-props-no-spreading": "off",
    "import/no-anonymous-default-export": "off",
    "react/no-array-index-key": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
